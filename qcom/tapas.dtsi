&tlmm {
/* Onewire */
	onewire_gpio_active: onewire_gpio_active {
			mux {
				pins = "gpio108";
				function = "gpio";
			};			

			config {
				pins = "gpio108";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

	onewire_gpio_sleep: onewire_gpio_sleep {
			mux {
				pins = "gpio108";
				function = "gpio";
			};
	
			config {
				pins = "gpio108";
				drive-strength = <2>;
				bias-pull-up;
			};
		};
};

